			<xs:element name="DokladDatum" type="rt:DokladDatumType"/>
			<xs:element minOccurs="1" name="DokladStatus" type="rt:DokladStatusType"/>
			<xs:element name="DokladDruh" type="rt:DokladDruhType"/>
